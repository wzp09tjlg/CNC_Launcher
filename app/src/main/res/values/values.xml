<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--侧滑相关-->
    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>
    <item name="selected_view" type="id" />
    <!--SlidingMenu相关-->
    <item name="horizontal_page_view" type="id" />

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundRectImage">
        <attr name="corners_radius" format="integer" />
        <attr name="image" format="reference" />
        <attr name="border_width" format="integer" />
    </declare-styleable>
    <!--FocusZoomUtil相关-->
    <item name="lb_focus_animator" type="id" />
    <item name="lb_focus_zoom_factor_llarge" type="fraction">150%</item>
    <item name="lb_focus_zoom_factor_large" type="fraction">140%</item>
    <item name="lb_focus_zoom_factor_huge" type="fraction">135%</item>
    <item name="lb_focus_zoom_factor_big" type="fraction">130%</item>
    <item name="lb_focus_zoom_factor_medium" type="fraction">125%</item>
    <item name="lb_focus_zoom_factor_small" type="fraction">120%</item>
    <item name="lb_focus_zoom_factor_litte" type="fraction">115%</item>
    <item name="lb_focus_zoom_factor_petty" type="fraction">110%</item>
    <!--gridview相关-->
    <declare-styleable name="lbBaseGridView">
        <attr name="focusOutFront" format="boolean" />
        <attr name="focusOutEnd" format="boolean" />
        <attr name="horizontalMargin" format="dimension" />
        <attr name="verticalMargin" format="dimension" />
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="lbHorizontalGridView">
        <attr name="rowHeight" format="dimension">
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="numberOfRows" format="integer" />
    </declare-styleable>
    <declare-styleable name="lbVerticalGridView">
        <attr name="columnWidth" format="dimension">
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="numberOfColumns" format="integer" />
    </declare-styleable>

    <item name="expandable_text" type="id" />
    <item name="expand_collapse" type="id" />

    <item name="rightSectionId" type="id" />
    <item name="rightItemId" type="id" />

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="animAlphaStart" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="selectView">
        <attr name="itemMargin" format="dimension" />
        <attr name="itemHeight" format="dimension" />
    </declare-styleable>
    <!--CircularProgressBar相关-->
    <declare-styleable name="CircularProgressBar">
        <attr name="cpbStyle" format="reference" />
        <attr name="cpb_color" format="color" />
        <attr name="cpb_colors" format="reference" />
        <attr name="cpb_stroke_width" format="dimension" />
        <attr name="cpb_min_sweep_angle" format="integer" />
        <attr name="cpb_max_sweep_angle" format="integer" />
        <attr name="cpb_sweep_speed" format="float" />
        <attr name="cpb_rotation_speed" format="float" />
    </declare-styleable>

    <color name="cpb_default_color">#33b5e5</color>
    <dimen name="cpb_default_stroke_width">4dp</dimen>
    <integer name="cpb_default_min_sweep_angle">20</integer>
    <integer name="cpb_default_max_sweep_angle">300</integer>
    <item name="cpb_default_sweep_speed" format="float" type="string">1</item>
    <item name="cpb_default_rotation_speed" format="float" type="string">1</item>
    <!--ShimmerFrameLayout相关-->
    <declare-styleable name="ShimmerFrameLayout">
        <attr name="auto_start" format="boolean" />
        <attr name="base_alpha" format="float" />
        <attr name="duration" format="integer" />
        <attr name="repeat_count" format="integer" />
        <attr name="repeat_delay" format="integer" />
        <attr name="repeat_mode" format="enum">
            <enum name="restart" value="1" />
            <enum name="reverse" value="2" />
        </attr>
        <attr name="angle" format="enum">
            <enum name="cw_0" value="0" />
            <enum name="cw_90" value="90" />
            <enum name="cw_180" value="180" />
            <enum name="cw_270" value="270" />
        </attr>
        <attr name="dropoff" format="float" />
        <attr name="fixed_width" format="dimension" />
        <attr name="fixed_height" format="dimension" />
        <attr name="intensity" format="float" />
        <attr name="relative_width" format="float" />
        <attr name="relative_height" format="float" />
        <attr name="shape" format="enum">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
        </attr>
        <attr name="tilt" format="float" />
    </declare-styleable>
</resources>